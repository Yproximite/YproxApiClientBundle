{
  "name": "yproximite/yprox-api-client-bundle",
  "type": "symfony-bundle",
  "description": "Integration of yProx API client library for Symfony",
  "license": "MIT",
  "authors": [
    {
      "name": "Nikita Sviridenko",
      "email": "mail.sviridenko@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "symfony/framework-bundle": "^3.4 || ^4.0 || ^5.0",
    "yproximite/yprox-api-client": "^0.11.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^6.0",
    "symfony/phpunit-bridge": "^3.4 || ^4.0 || ^5.0",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpstan/phpstan": "^0.12.53",
    "phpstan/phpstan-phpunit": "^0.12.16",
    "phpstan/phpstan-strict-rules": "^0.12.5"
  },
  "autoload": {
    "psr-4": {
      "Yproximite\\Bundle\\YproxApiClientBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yproximite\\Bundle\\YproxApiClientBundle\\Tests\\": "tests"
    }
  }
}
